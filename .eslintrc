{
  "globals": {
    "jQuery": true,
    "$": true,
    "describe": false,
    "it": false,
    "expect": false,
    "$on": false,
    "beforeEach": false
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": ["semistandard"],
  "rules": {
      "indent": 0,
      "no-tabs": 0,
      "one-var": 0,
      "no-path-concat": 0,
      "eol-last": 0,
      "valid-jsdoc": "warn",
      "spaced-comment": 0,
  }
}
