{
  "name": "Queensland-Government-Web-Template",
  "version": "4.0.0",
  "subVersion": "4.0.0",
  "wt-version": "4.0.0",
  "description": "This template is designed to provide a template for all Franchise websites, and the underpinning technology for new Agency websites.",
  "main": "gulpfile.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/qld-gov-au/qg-web-template.git"
  },
  "scripts": {
    "build": "gulp build && node gulp/build-tasks/node-docs-flatten.js && gulp wt-updateVersion",
    "test:e2e": "jest --detectOpenHandles",
    "release": "export NODE_ENV=prod && npm run build && gulp release && node gulp/release-tasks/node-template-flatten.js && gulp replace-links",
    "release:test": "export NODE_ENV=test && npm run build && gulp release && node gulp/release-tasks/node-template-flatten.js && gulp replace-links",
    "start": "npm run build && gulp watch",
    "watch": "gulp watch",
    "publish-npm": "export NODE_ENV=prod && gulp wt-clean && gulp wt-clone && gulp wt-sync && gulp wt-updateVersion && gulp wt-npm && gulp wt-add && gulp wt-commit && gulp wt-tag && gulp wt-push",
    "publish-test": "npm run release:test && gulp cdn-clean && gulp cdn-clone && gulp cdn-branch && gulp cdn-transfer && gulp cdn-add && gulp cdn-commit && gulp cdn-push",
    "create-swe-release": "gulp swe-add && gulp swe-tag && gulp swe-commit && gulp swe-push",
    "publish-cdn": "gulp cdn-clean && gulp cdn-clone && gulp cdn-transfer && gulp cdn-add && gulp cdn-commit && gulp cdn-push",
    "publish-npm-cdn": "npm run publish-npm && npm run publish-cdn"
  },
  "authors": "Queensland Government",
  "licence": "all rights reserved",
  "homepage": "http://www.qld.gov.au/",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^4.0.1",
    "core-js": "^3.6.4",
    "css-loader": "^0.28.1",
    "cssnano": "^3.10.0",
    "debug": "^4.1.1",
    "del": "^2.2.2",
    "dotenv": "^4.0.0",
    "eslint": "^6.0.1",
    "eslint-config-semistandard": "^7.0.0",
    "eslint-config-standard": "^6.0.1",
    "eslint-html-reporter": "^0.5.2",
    "eslint-loader": "^1.4.1",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-standard": "^2.0.0",
    "event-stream": "^3.3.4",
